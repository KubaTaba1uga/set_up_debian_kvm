#!/bin/sh
# See the `DISPATCHER SCRIPTS` section of `man NetworkManager`.
# Remember to make this file executable!
[ "$1" != "virtbr0nat" ] && exit 0
case "$2" in
    "up")
        /bin/systemctl start dnsmasq@virtbr0nat.service || :
        ;;
    "down")
        /bin/systemctl stop dnsmasq@virtbr0nat.service || :
        ;;
esac
